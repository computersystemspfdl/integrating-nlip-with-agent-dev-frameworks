# Environment Variables for NLIP Agent Frameworks Demo
# Copy this file to .env and fill in your API keys

# OpenRouter API (for both LangChain and LlamaIndex)
# Get your key from: https://openrouter.ai/
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Server Configuration
LANGCHAIN_PORT=8012
LLAMAINDEX_PORT=8013
STANDALONE_LANGCHAIN_PORT=8014
STANDALONE_LLAMAINDEX_PORT=8015

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=simple
